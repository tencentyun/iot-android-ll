    <declare-styleable name="StatusBarHeightView">
        <attr format="integer" name="use_type">
            <enum name="use_height" value="0"/>
            <enum name="use_padding_top" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="TabLine">
        <attr format="color" name="tab_line_color"/>
        <attr format="reference" name="tab_src"/>
        <attr format="dimension" name="tab_line_height"/>
        <attr format="dimension" name="tab_width"/>
        <attr format="enum" name="tab_line_gravity">
            <enum name="center" value="0"/>
            <enum name="top" value="1"/>
            <enum name="bottom" value="2"/>
        </attr>
        <attr format="dimension" name="tab_height"/>
        <attr format="integer" name="tab_count"/>
        <attr format="dimension" name="tab_padding_start"/>
        <attr format="dimension" name="tab_padding_end"/>
        <attr format="boolean" name="tab_round"/>
        <attr format="boolean" name="tab_smooth_scroll"/>
