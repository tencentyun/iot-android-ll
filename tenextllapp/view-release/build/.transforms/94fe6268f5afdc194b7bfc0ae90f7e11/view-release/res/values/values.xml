    <string name="email_invalid">邮箱地址格式不正确</string>
    <string name="email_null">请输入邮箱地址</string>
    <string name="mobile_phone_number_invalid">手机号格式不正确</string>
    <string name="mobile_phone_number_null">请输入手机号</string>
