    <string name="toast_login">密码修改成功，请重新登录</string>
    <string name="toast_password_not_verify">两次输入的密码不一致</string>
    <declare-styleable name="BgView">
        <attr format="dimension" name="bg_height"/>
        <attr format="color" name="bg_color"/>
    </declare-styleable>
    <declare-styleable name="CRecyclerView">
        <attr format="dimension" name="max_height"/>
    </declare-styleable>
    <declare-styleable name="CView">
        <attr format="string" name="c_text"/>
        <attr format="dimension" name="c_textSize"/>
        <attr format="enum" name="c_textStyle">
            <enum name="normal" value="0"/>
            <enum name="bold" value="1"/>
        </attr>
        <attr format="enum" name="c_gravity">
            <enum name="center" value="0"/>
            <enum name="top" value="1"/>
            <enum name="bottom" value="2"/>
            <enum name="left" value="3"/>
            <enum name="right" value="4"/>
        </attr>
        <attr format="color" name="c_textColor"/>
        <attr format="reference" name="c_textDrawable"/>
    </declare-styleable>
    <declare-styleable name="CircleProgressView">
        <attr format="dimension" name="progress_text_size"/>
        <attr format="dimension" name="unit_text_size"/>
        <attr format="dimension" name="inner_width"/>
        <attr format="color" name="inner_color"/>
        <attr format="dimension" name="center_width"/>
        <attr format="color" name="center_color"/>
        <attr format="dimension" name="outer_width"/>
        <attr format="color" name="outer_top_color"/>
        <attr format="color" name="outer_bottom_color"/>

        <attr format="dimension" name="outer_dash_width"/>
        <attr format="dimension" name="outer_and_center_gap"/>
        <attr format="dimension" name="center_out_line_width"/>
        <attr format="color" name="center_out_line_color"/>
        <attr format="integer" name="progress"/>
    </declare-styleable>
    <declare-styleable name="HistogramView">
        <attr format="dimension" name="histogram_width"/>
        <attr format="reference" name="histogram_background"/>
        <attr format="dimension" name="histogram_gap"/>
        <attr format="boolean" name="histogram_can_scroll"/>
    </declare-styleable>
    <declare-styleable name="IconTextView">
        <attr format="reference" name="icon_src"/>
        <attr format="enum" name="icon_gravity">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
        </attr>
        <attr format="dimension" name="icon_padding"/>
        <attr format="dimension" name="icon_width"/>
        <attr format="dimension" name="icon_height"/>
        <attr format="enum" name="icon_type">
            <enum name="none" value="0"/>
            <enum name="triangle" value="1"/>
            <enum name="circle" value="2"/>
        </attr>
        <attr format="dimension" name="icon_stroke_width"/>
        <attr format="color" name="icon_color"/>
        <attr format="enum" name="icon_style">
            <enum name="stroke" value="0"/>
            <enum name="fill" value="1"/>
        </attr>
        <attr format="enum" name="icon_triangle_direction">
            <enum name="top" value="0"/>
            <enum name="bottom" value="1"/>
            <enum name="left" value="2"/>
            <enum name="right" value="3"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="RingProgressView">
        <attr format="reference" name="ring_background"/>
        <attr format="reference" name="ring_seek_background"/>
        <attr format="dimension" name="ring_seek_width"/>
        <attr format="integer" name="ring_progress"/>
    </declare-styleable>
    <declare-styleable name="SeekProgress">
        <attr format="color|reference" name="seek_progress_background"/>
        <attr format="color|reference" name="seek_first_color"/>
        <attr format="color|reference" name="seek_bar_color"/>
        <attr format="color" name="seek_text_color"/>
        <attr format="color|reference" name="seek_bar_shape_color"/>
