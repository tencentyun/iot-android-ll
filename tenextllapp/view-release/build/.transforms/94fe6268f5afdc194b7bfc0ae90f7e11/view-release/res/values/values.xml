        <attr format="dimension" name="seek_bar_radius"/>
        <attr format="integer" name="seek_progress_min"/>
        <attr format="integer" name="seek_progress_max"/>
        <attr format="integer" name="seek_progress_step"/>
        <attr format="boolean" name="seek_show_text"/>
