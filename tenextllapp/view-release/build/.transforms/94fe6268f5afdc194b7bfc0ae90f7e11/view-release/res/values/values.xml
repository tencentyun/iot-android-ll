        <attr format="integer" name="length"/>
        <attr format="reference" name="selected_background"/>
        <attr format="reference" name="unselected_background"/>
        <attr format="dimension" name="verify_width"/>
        <attr format="dimension" name="verify_height"/>
        <attr format="dimension" name="text_size"/>
        <attr format="color" name="text_color"/>
        <attr format="boolean" name="text_bold"/>
    </declare-styleable>
    <declare-styleable name="WaveProgress">
        <attr format="integer" name="wave_progress"/>
        <attr format="color|reference" name="wave_background"/>
        <attr format="color|reference" name="wave_first_background"/>
        <attr format="color|reference" name="wave_second_background"/>
        <attr format="dimension" name="wave_height"/>
        <attr format="dimension" name="wave_width"/>
        <attr format="integer" name="wave_duration"/>
    </declare-styleable>
</resources>